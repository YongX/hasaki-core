{
  "name": "hasaki",
  "templateRootPath": "./_template",
  "placeholder": "",
  "rules": [
    {
      "containerRule": {
        "extension": "js",
        "suffix": "",
        "prefix": "_",
        "path": "./container/",
        "template": "container.js"
      }
    },
    {
      "pageRule": {
        "templates": ["app/pages/Appointment"]
      }
    },
    {
      "sagaRule": {
        "templates": ["app/pages/Appointment", "_template/action.js", {
          "source": "app/saga/appointment",
          "placeholder": "appointment",
          "keepFileName": false
        }]
      }
    },
    {
      "actionRule": {
        "extension": "js",
        "suffix": "",
        "path": "./actions/",
        "template": "action.js"
      }
    },
    {
      "sagaRule": {
        "extension": "js",
        "suffix": "saga",
        "path": "./saga/",
        "content": "haha"
      }
    },
    {
      "rule3": [
        "actionRule",
        "sagaRule",
        "containerRule"
      ]
    }
  ]
}
