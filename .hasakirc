{
  "name": "hasaki",
  "templateRootPath": "./_template",
  "placeholder": "",
  "rules": [
    {
      "actionRule": {
        "extension": "js",
        "suffix": "",
        "prefix": "_",
        "path": "./container/",
        "template": "container.js"
      }
    },
    {
      "pageRule": {
        "templates": [
          "app/pages/Appointment"
        ]
      }
    },
    {
      "sagaRule": {
        "templates": [
          {
            "source": "app/saga/appointment",
            "placeholder": "appointment",
            "keepFileName": false
          },
          "app/actions/appointment.js"
        ]
      }
    },
    {
      "actionRule": {
        "extension": "js",
        "suffix": "",
        "path": "./actions/",
        "template": "action.js"
      }
    },
    {
      "rule3": [
        "actionRule",
        "sagaRule",
        "containerRule"
      ]
    }
  ]
}
